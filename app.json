{
  "expo": {
    "name": "ace-fi",
    "slug": "ace-fi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*",
      "src/assets/fonts/*"
    ],
    "fonts": [
      {
        "asset": "./src/assets/fonts/SF-Pro-Display-Regular.otf",
        "family": "SFProDisplay"
      },
      {
        "asset": "./src/assets/fonts/SF-Pro-Display-Medium.otf",
        "family": "SFProDisplay-Medium"
      },
      {
        "asset": "./src/assets/fonts/SF-Pro-Display-Bold.otf",
        "family": "SFProDisplay-Bold"
      },
      {
        "asset": "./src/assets/fonts/SF-Pro-Text-Regular.otf",
        "family": "SFProText"
      },
      {
        "asset": "./src/assets/fonts/SF-Pro-Text-Bold.otf",
        "family": "SFProText-Bold"
      }
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
